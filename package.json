{
  "name": "huc_remaster",
  "productName": "huc_remaster",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "./src/main.js",
  "homepage": ".",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "estart": "electron .",
    "rstart": "react-scripts start",
    "tstart": "npx tailwindcss -i ./src/input.css -o ./src/MainView.css --watch",
    "astart": "react-scripts start & npx tailwindcss -i ./src/input.css -o ./src/MainView.css --watch & electron .",
    "epackage": "electron-forge package",
    "emake": "electron-forge make",
    "tmppackage": "electron-packager .",
    "eppackage": "electron-packager ~/HUC_Remaster HUC_Remaster --all --out ~/ --electron-version 29.1.4"
  },
  "keywords": [],
  "author": "Woisol-G",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "huc_remaster"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "huc_remaster"
      }
    }
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "electron": "^29.1.4",
    "electron-compile": "^6.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.7.1",
    "mysql": "2.18.1",
    "path": "0.12.7",
    "url": "0.11.3"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.26",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "tailwindcss": "3.4.1"
  }
}